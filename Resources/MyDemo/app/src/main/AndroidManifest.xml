<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light"
        tools:targetApi="31">
        <activity
            android:name=".activity.SplashActivity"
            android:exported="true"
            android:theme="@style/Theme.AppCompat.Light">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AppMainActivity" />



        <activity android:name=".activity.SecondActivity" />
        <activity android:name=".activity.MainActivityLifecycle" />
        <activity android:name=".ui.ToastActivity" />
        <activity android:name=".ui.recyclerview.RecyclerViewActivity" />
        <activity android:name=".ui.dialog.DialogDemoActivity" />
        <activity android:name=".ui.layout.LayoutActivity" />
        <activity android:name=".ui.layout.ConstraintLayoutActivity" />
        <activity android:name=".ui.layout.GridLayoutActivity" />
        <activity android:name=".ui.layout.LinearLayoutActivity" />
        <activity android:name=".ui.layout.RelativeLayoutActivity" />
        <activity android:name=".ui.layout.FrameLayoutActivity" />
        <activity android:name=".ui.layout.action.GuideLineActivity" />
        <activity android:name=".data.sp.SharePreferenceActivity" />
        <activity android:name=".data.database.SQLiteActivity" />
        <activity android:name=".activity.ImplicitIntentActivity" />
        <activity android:name=".ui.drawable.DrawableActivity" />

        <activity android:name=".activity.DemoActivity" />

    </application>

</manifest>